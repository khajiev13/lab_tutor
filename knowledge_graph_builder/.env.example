# Google AI API Key for LLM (legacy)
GOOGLE_API_KEY=your_google_api_key_here

# XiaoCase AI API Configuration
XIAOCASE_API_KEY=your_xiaocase_api_key_here
XIAOCASE_API_BASE=https://api.xiaocaseai.com/v1
XIAOCASE_MODEL=gemini-2.0-flash

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# LangSmith Configuration for Tracing and Debugging
LANGCHAIN_TRACING_V2=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=concept-relationship-detection

# Optional: LangSmith endpoint (defaults to https://api.smith.langchain.com)
# LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
